{
  "version": 2,
  "name": "sort-me",
  "alias": "sort-me.space",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next",
      "config": {
        "maxLambdaSize": "25mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/posts/(?<slug>[^/]+)$",
      "dest": "/post?slug=$slug"
    },
    {
      "src": "/write/(?<slug>[^/]+)$",
      "dest": "/write?slug=$slug"
    }
  ]
}
